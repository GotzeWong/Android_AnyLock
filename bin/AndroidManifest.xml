<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.jiangnan.isnc.wsn.anylock"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_anylock"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cn.jiangnan.isnc.wsn.anylock.ItemListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cn.jiangnan.isnc.wsn.anylock.ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName="cn.jiangnan.isnc.wsn.anylock.ItemListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="cn.jiangnan.isnc.wsn.anylock.ItemListActivity" />
        </activity>
    </application>

</manifest>
